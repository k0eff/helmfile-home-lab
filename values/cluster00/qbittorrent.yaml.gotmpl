ingress:
  main:
    enabled: true
    ingressClassName: nginx
    annotations:
      cert-manager.io/cluster-issuer: cloudflare-cluster00
    hosts:
      - host: qbittorrent.koeff.com
        paths:
          - path: /
            pathType: Prefix
            service:
              name: qbittorrent
              port: 8080
    tls:
      - secretName: cloudflare-cluster00-qbittorrent
        hosts:
          - qbittorrent.koeff.com
