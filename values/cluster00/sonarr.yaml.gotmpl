ingress:
  main:
    enabled: true
    ingressClassName: nginx
    annotations:
      cert-manager.io/cluster-issuer: cloudflare-cluster00
    hosts:
      - host: sonarr.koeff.com
        paths:
          - path: /
            pathType: Prefix
            service:
              name: sonarr
              port: 8989
    tls:
      - secretName: cloudflare-cluster00-sonarr
        hosts:
          - sonarr.koeff.com
